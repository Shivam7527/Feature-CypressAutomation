{
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.1.0",
    "@cypress/browserify-preprocessor": "latest",
    "cypress": "^14.4.1",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-mysql": "^1.0.4",
    "esbuild": "^0.25.5",
    "exceljs": "^4.4.0",
    "multiple-cucumber-html-reporter": "^3.9.2",
    "neat-csv": "5.1.0",
    "puppeteer": "^24.10.1"
  },
  "scripts": {
    "test": "npx cypress run --reporter mochawesome --reporter-options reportDir=cypress/results,reportFilename=mochawesome,overwrite=true,html=true,json=true",
    "generate-html": "npx mochawesome-merge cypress/results/*.json | npx mochawesome-report-generator --reportDir cypress/results --reportFilename mochawesome",
    "browser-test": "npx cypress run --spec 'cypress/Integration/Example/Text5.js' --browser chrome",
    "generate:pdf": "node generate-pdf.js"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true,
      "output": "cypress/cucumberReports/results.json"
    }
  },
  "dependencies": {
    "mochawesome-report-generator": "^6.2.0"
  }
}
